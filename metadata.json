{
  "name": "systemsathomesdotcom/ssh",
  "version": "2.2.1",
  "author": "Systems <systems@homes.com>",
  "summary": "puppet module to manage ssh",
  "license": "MIT",
  "source": "https://github.com/systemsathomesdotcom/puppet-openssh",
  "project_page": "https://github.com/systemsathomesdotcom/puppet-openssh",
  "issues_url": "https://github.com/systemsathomesdotcom/puppet-openssh/issues",
  "description": "This module allows managing ssh",
  "tags": ["ssh", "openssh", "hostkey", "ssh_keygen"],
  "operatingsystem_support": [
    {
        "operatingsystem": "CentOS",
        "operatingsystemrelease": [
            "5",
            "6",
            "7"
        ]
    },
    {
        "operatingsystem": "Debian",
        "operatingsystemrelease": [
            "5",
            "6",
            "7"
        ]
    },
    {
        "operatingsystem": "OracleLinux",
        "operatingsystemrelease": [
            "6.5",
        ]
    },
    {
        "operatingsystem": "Ubuntu",
        "operatingsystemrelease": [
            "12.04",
            "14.04",
        ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=2.7.11"
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">=1.0.0" }
  ]
}
